{
  "applications": {
    "gecko": {
      "id": "srbTranslit@srbodroid.com"
    }
  },
  "manifest_version": 2,
  "name": "srbTranslit",
  "version": "1.1",
  "permissions": [
    "activeTab",
    "webNavigation",
    "<all_urls>"
  ],
  "description": "Transliterates Serbian Cyrilic to Serbian Latin on any web page",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "is-on.png"
  },
  "browser_action": {
    "default_icon": "is-on.png",
    "default_title": "srbTranslit",
    "browser_style": true
  }
}